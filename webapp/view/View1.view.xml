<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="KT_DEMO_NP.controller.View1">
	<App>
		<pages>
			<Page title="Article Status">
				<content>
					<VBox direction="Column" id="vbox0" justifyContent="Center" width="100%">
						<items>
							<Label id="label3" textAlign="Center" width="100%" text="Articles Changed Today....."/>
						</items>
					</VBox>
					<Table id="table0" items="{/Entity1_Set}" noDataText="Drop column list items here and columns in the area above">
						<items>
							<ColumnListItem counter="0" id="item1">
								<cells>
									<Text id="text5" maxLines="0" text="{ArticleID}"/>
									<Text id="text6" maxLines="0" text="{StatusType}"/>
									<Text id="text7" maxLines="0" text="{StautsCode}"/>
									<Text text="{StatusDate}" id="__text6"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column id="column0">
								<header>
									<Label id="label0" text="Article ID"/>
								</header>
							</Column>
							<Column id="column1">
								<header>
									<Label id="label1" text="Status Type"/>
								</header>
							</Column>
							<Column id="column2">
								<header>
									<Label id="label2" text="Status Value"/>
								</header>
							</Column>
							<Column id="__column0">
							    <header>
							        <Text text="Status Date" id="__text5"/>
							    </header>
							</Column>
						</columns>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>